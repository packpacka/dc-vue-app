{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": [
      "../node_modules/@types",
      "./types"
    ],
    "types": [
      "vue",
      "axios",
      "node",
      "jasmine",
      "ymaps"
    ],
    "lib": ["es6", "dom"]
  },
  "exclude": [
    "node_modules"
  ]
}
